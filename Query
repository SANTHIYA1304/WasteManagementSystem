CREATE TABLE ZONE_TBL_NEW (
    ZONE_ID VARCHAR2(10) PRIMARY KEY,
    ZONE_NAME VARCHAR2(150),
    ZONE_TYPE VARCHAR2(50),
    AREA_SQKM NUMBER(6,2),
    STATUS VARCHAR2(20)
);
CREATE TABLE VEHICLE_TBL_NEW (
    VEHICLE_ID VARCHAR2(10) PRIMARY KEY,
    VEHICLE_TYPE VARCHAR2(50),
    CAPACITY_KG NUMBER(10),
    SUITABILITY VARCHAR2(50),
    STATUS VARCHAR2(20)
);

CREATE TABLE PICKUP_TBL_NEW (
    PICKUP_ID NUMBER(10) PRIMARY KEY,
    ZONE_ID VARCHAR2(10),
    VEHICLE_ID VARCHAR2(10),
    PICKUP_DATE DATE,
    START_TIME VARCHAR2(10),
    EXPECTED_VOLUME_KG NUMBER(10),
    ACTUAL_VOLUME_KG NUMBER(10),
    STATUS VARCHAR2(20),
    FOREIGN KEY (ZONE_ID) REFERENCES ZONE_TBL_NEW(ZONE_ID),
    FOREIGN KEY (VEHICLE_ID) REFERENCES VEHICLE_TBL_NEW(VEHICLE_ID)
);
CREATE SEQUENCE PICKUP_SEQ_NEW
    START WITH 70001
    INCREMENT BY 1
    NOCACHE
    NOCYCLE;
    
    
SELECT * FROM ZONE_TBL_NEW;
SELECT * FROM VEHICLE_TBL_NEW;
SELECT * FROM PICKUP_TBL_NEW;

